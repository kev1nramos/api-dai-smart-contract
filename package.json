{
  "name": "tessera-senior-backend-coding-challenge",
  "version": "1.0.0",
  "main": "src/main.ts",
  "repository": "git@github.com:kev1nramos/tessera-senior-backend-coding-challenge.git",
  "author": "kev1nramos <kevinalmeidaramos@gmail.com>",
  "license": "MIT",
  "scripts": {
    "pre-commit": "yarn lint-staged",
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "build": "tsc",
    "start": "yarn run start:dev",
    "start:dev": "yarn run build && cross-env NODE_ENV=development ts-node-dev src/index.ts --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/index.js",
    "test": "jest --runInBand --forceExit",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli",
    "typeorm:migrate": "yarn run typeorm migration:run -d src/database/typeorm.config.ts",
    "typeorm:generate-migration": "yarn run typeorm:migrate && yarn run typeorm -d src/database/typeorm.config.ts migration:generate",
    "typeorm:create-migration": "yarn run typeorm:migrate && yarn run typeorm migration:create",
    "typeorm:revert-migration": "yarn run typeorm -d src/database/typeorm.config.ts migration:revert",
    "typeorm:migrate-prod": "yarn run typeorm migration:run -d dist/database/typeorm.config.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^6.0.0",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "morgan": "^1.10.0",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "typeorm": "^0.3.12",
    "web3": "^1.9.0",
    "winston": "^3.8.2"
  }
}
